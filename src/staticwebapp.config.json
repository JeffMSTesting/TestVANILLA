{
    "trailingSlash": "auto",
    "routes": [
        {
            "route": "/",
            "rewrite": "/index.html",
            "statusCode": 302
        },
        {
            "route": "/index.html",
            "rewrite": "/index.html",
            "statusCode": 302
        },
        {
            "route": "/en/*",
            "rewrite": "/en/index.html",
            "statusCode": 200
        },
    ],
    "navigationFallback": {
        "rewrite": "/index.html",
        "exclude": [
            "/images/*.{png,jpg,gif,svg}",
            "/css/*",
            "/js/*"
        ]
    },
    "globalHeaders": {
        "Content-Security-Policy": "default-src 'self'"
    }
}
